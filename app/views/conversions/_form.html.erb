<%= form_for @conversion do |f| %>
  <p>
    <%= f.label :english %><br />
    <%= f.text_field :english %>
  </p>
  <p>
    <%= f.label :phonetic %><br />
    <%= f.text_field :phonetic %>
  </p>
  <p>
    <%= f.label :level  %><br />
    <%= f.select :level, (1..8).to_a.map{|i| [i,i]}, :include_blank => true %>
  </p>
  Owned by <%= @conversion.user.username rescue "no one" %>
  <p><%= f.submit %></p>
<% end %>
